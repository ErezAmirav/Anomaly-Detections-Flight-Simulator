<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="View.files.MyFilesController">

    <MenuBar prefHeight="30.0" prefWidth="1000" BorderPane.alignment="CENTER">
        <menus>
            <Menu mnemonicParsing="false" text="Open">
                <items>
                    <MenuItem fx:id="csv" mnemonicParsing="false" onAction="#openCSVFile" text="Choose a Flight CSV File" />
                    <MenuItem fx:id="algo" mnemonicParsing="false" onAction="#openAlgFile" text="Choose an Algorithm File" />
                </items>
            </Menu>
        </menus>
    </MenuBar>

</AnchorPane>
